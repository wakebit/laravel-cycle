{
    "name": "wakebit/laravel-cycle",
    "description": "Cycle ORM integration with Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "wakebit/cycle-bridge": "^2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "orchestra/testbench": "^4.8 || ^5.2 || ^6.0 || ^7.0 || ^8.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "vimeo/psalm": "^4.22"
    },
    "autoload": {
        "psr-4": {
            "Wakebit\\LaravelCycle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wakebit\\LaravelCycle\\TestsApp\\": "tests/App",
            "Wakebit\\LaravelCycle\\Tests\\": "tests/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wakebit\\LaravelCycle\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "minimum-stability": "dev",
        "prefer-stable": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
